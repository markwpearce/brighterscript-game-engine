{
  "plugins": [
    "plugins/markdown",
    "./node_modules/brighterscript-jsdocs-plugin/convert-brighterscript-docs.js"
  ],
  "source": {
    "include": [
      "src",
      "README.md"
    ],
    "includePattern": ".+\\.br?s$"
  },
  "templates": {
    "referenceTitle": "Brighterscript Game Engine",
    "collapse": true
  },
  "opts": {
    "recurse": true,
    "template": "node_modules/braintree-jsdoc-template",
    "brighterscript-jsdocs-plugin": {
      "addModule": false
    }
  }
}