namespace BGE.Math

  interface Transform
    position as BGE.Math.Vector
    rotation as BGE.Math.vector
    scale as BGE.Math.Vector
  end interface


  function createTransform() as Transform
    return {
      position: BGE.Math.VectorOps.create()
      rotation: BGE.Math.VectorOps.create()
      scale: createScaleVector(1)
    }
  end function


end namespace