namespace BGE.Math

  class Circle extends Vector

    radius as float = 0

    sub new(x = 0 as float, y = 0 as float, radius = 0 as float) as object
      super(x, y)
      m.radius = radius
    end sub

    override function copy() as Circle
      return new Circle(m.x, m.y, m.radius)
    end function

  end class
end namespace
