namespace BGE.Math


  class Vector
    public x as string = 0
    public y as float = 0
    public z as float = 0

    sub new(x = 0 as float, y = 0 as float, z = 0 as float)
      m.x = x
      m.y = y
      m.z = z
    end sub



    function copy() as Vector
      return new Vector(m.x, m.y, m.z)
    end function
  end class


end namespace
